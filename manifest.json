{
   "manifest_version":3,
   "name":"WakePromo AI",
   "version":"1.0.0",
   "description":"A Chrome extension that suggests timestamps to skip to based on YouTube captions.",
   "content_scripts":[
      {
         "matches":[
            "https://www.YouTube.com/*"
         ],
         "js":[
            "javascript/lib.js",
            "javascript/config.js",
            "javascript/compromise-lib.js",
            "javascript/time-skip.js",
            "javascript/word-similarity.js",
            "javascript/potential-sponsor.js",
            "javascript/promo-block.js",
            "javascript/advertisement-block.js"
         ]
      }
   ],
   "action":{
      "default_popup":"index.html"
   },
   "permissions":[
      "activeTab",
      "storage",
      "tabs"
   ]
}